<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Ui/etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction  name="listing_massaction">
            <action name="exportgooglefeed">
                <settings>
                    <confirm>
                        <message translate="true">Export selected items on Google Feed?</message>
                        <title translate="true">Export items on Google Feed</title>
                    </confirm>
                    <url path="googleshoppingfeed/products/selectedproprofiler"/>
                    <type>exportgooglefeed</type>
                    <label translate="true">Export items on Google Feed</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
