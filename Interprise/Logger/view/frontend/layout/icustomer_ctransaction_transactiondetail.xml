<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
   <update handle="customer_account"/>
    <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Transaction Detail</argument>
            </action>
     </referenceBlock>
    <referenceContainer name="content">
        <block class="Interprise\Logger\Block\Icustomer\Transactiondetail" name="icustomer_ctransaction_transactiondetail" template="Interprise_Logger::ictransaction_detail.phtml" cacheable="false" />
    </referenceContainer>

</page>
