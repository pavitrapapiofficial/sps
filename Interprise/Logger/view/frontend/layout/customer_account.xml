            </arguments>
         </block>
         <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-interprisecrm">
            <arguments>
               <argument name="path" xsi:type="string">icustomer/ccrm/index</argument>
               <argument name="label" xsi:type="string">CRM/Cases</argument>
            </arguments>
         </block>
         <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-interprisereturns">
            <arguments>
               <argument name="path" xsi:type="string">icustomer/creturns/index</argument>
               <argument name="label" xsi:type="string">Returns</argument>
            </arguments>
         </block>
         <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-interprisestatement">
            <arguments>
               <argument name="path" xsi:type="string">icustomer/cstatement/index</argument>
               <argument name="label" xsi:type="string">Account Statement</argument>
            </arguments>
         </block>
