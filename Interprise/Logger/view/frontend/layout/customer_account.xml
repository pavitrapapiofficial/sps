    <body>
     <referenceBlock class="Magento\Customer\Block\Account\Navigation" name="customer_account_navigation">

        
         <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-interpriseorders">
            <arguments>
               <argument name="path" xsi:type="string">icustomer/corder/index</argument>
               <argument name="label" xsi:type="string">Orders</argument>
               <argument name="sortOrder" xsi:type="number">110</argument>
            </arguments>
         </block>
          <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-interprisequote">
                <arguments>
                    <argument name="path" xsi:type="string">icustomer/cquote/index</argument>
                    <argument name="label" xsi:type="string">Quote</argument>
                </arguments>
        </block>
         <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-interprisetransaction">
